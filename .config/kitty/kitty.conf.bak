# BEGIN_KITTY_THEME
# Catppuccin-Mocha
include current-theme.conf
# END_KITTY_THEME
font_size 11.25
disable_ligatures always

cursor_shape block
shell_integration no-cursor
map ctrl+shift+n no_op

# BEGIN_KITTY_FONTS
font_family      family="CommitMono Nerd Font"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
